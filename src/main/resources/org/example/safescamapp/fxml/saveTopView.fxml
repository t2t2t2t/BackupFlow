<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="50.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.safescamapp.controllers.SmallViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="60.0" prefWidth="150.0" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <Button fx:id="onBackupButtonClick" mnemonicParsing="false" onAction="#onBackupButtonClick" text="Create Backup" />
            <Button fx:id="onReturnButtonClick" mnemonicParsing="false" onAction="#onReturnButtonClick" text="Return Backup" />
         </children>
      </HBox>
   </children>
</AnchorPane>
